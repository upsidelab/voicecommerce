  "keywords": [
    "ecommerce",
    "amazon",
    "alexa",
    "google",
    "assistant"
  ],
  "author": "Upside <hello@upsidelab.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.2.0",
    "mocha": "^5.2.0",
    "sinon": "^6.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "express-oauth-server": "^2.0.0",
    "pug": "^2.0.3"
  }
